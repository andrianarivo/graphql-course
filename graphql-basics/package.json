{
  "name": "graphql-basics",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "start": "node lib/index.js",
    "watch:nodemon": "nodemon lib/index.js",
    "watch:codegen": "graphql-codegen --config codegen.ts --watch \"src/schema.graphql\"",
    "watch:babel": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline --watch",
    "watch": "concurrently npm:watch:*",
    "gen": "graphql-codegen --config codegen.ts"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@graphql-tools/load-files": "^7.0.0",
    "@parcel/watcher": "^2.3.0",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.1",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.0.0",
    "uuid": "^9.0.1"
  }
}
